    <div class="main"> 
      <% if (Array.isArray(colorPalette) && colorPalette.length !== 0) { %>
      <section>
        <h2 id="color-palette" class="psg-h2 -boldweight">Base Colours</h2>
        <p class="small">You can use background prefixing bg-your-colour-name</p>
        <div class="contents">
          <div class="colours flex">
            <% colorPalette.forEach(function (palette) { %>
            <div class="item colourbox" onclick="copyToClipboard('<%= palette.name %>')">
              <div class="colourbox-sample" style="background-color: <%= palette.color %>">
              </div>
              <div class="colourbox-name" id="<%= palette.name %>">
                <%= palette.name %>
              </div>
            </div>
            <% }) %>
          </div>
        </div>
      </section>
      <% } %>

      <% maps.forEach(function (map) { %>
        <% if(map.link.title.indexOf('Layout') !== -1){ %>
          <styleguide-module id="layout">
            <% if (!showCode) { %> 
              <%- map.html %>
            <% } %>
          </styleguide-module>
        <% } else if(map.link.title.indexOf('Module') !== -1){ %>
          <styleguide-module id="module">
            <% if (!showCode) { %> 
              <%- map.html %>
            <% } %>
          </styleguide-module>
        <% } else{ %>
          <styleguide-module id="<%= map.link.title %>">
              <% if (!showCode) { %> 
              <%- map.html %>
              <% } %>
          </styleguide-module>
        <% } %>
      <% }) %>
        
     
    
    </div>
  </div>


    

  var sharedObservable = riot.observable();
  riot.mount('styleguide-module', {
    state: 'resting',
    observable: sharedObservable
    }
  );

  function showModule(e, target){
    sharedObservable.trigger('showComponent', target);
    console.log(e);
    e.preventDefault();
    return false;
  }
</script>
